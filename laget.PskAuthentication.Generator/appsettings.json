{
  "Security": {
    "RijndaelIV": "#{RijndaelIV}",
    "RijndaelKey": "#{RijndaelKey}",
    "Salt": "#{Salt}",
    "Secret": "#{Secret}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ]
  }
}